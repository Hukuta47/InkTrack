<Window x:Class="Request_Refill.Windows.WindowTraySelectFuntion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Request_Refill.Windows"
        mc:Ignorable="d"
        Title="WindowTraySelectFuntion" SizeToContent="WidthAndHeight">
    <Border Background="White" CornerRadius="10" Padding="20">
        <StackPanel>
            <TextBlock Text="Создание завязки на заправку" FontSize="16" Margin="0,0,0,20"/>
            <Button Content="Настройки" Margin="0,5"/>
            <Button Content="Закрыть программу" Margin="0,5"/>
            <Button Content="Печать" Margin="0,5"/>
        </StackPanel>
    </Border>
</Window>
